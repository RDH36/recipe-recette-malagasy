{
  "expo": {
    "name": "Tsikonina",
    "slug": "recipe-recette-malagasy",
    "version": "0.2.3",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Cette application a besoin d'accéder à votre galerie pour sauvegarder les images de recettes.",
        "NSUserTrackingUsageDescription": "Cette app utilise les notifications pour vous alerter des nouvelles recettes et événements."
      },
      "bundleIdentifier": "com.rdh36.reciperecettemalagasy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ],
      "notification": {
        "icon": "./assets/icons/notification.png",
        "color": "#FF8050"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.rdh36.reciperecettemalagasy"
    },
    "plugins": [
      "expo-router",
      "expo-media-library",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/icons/splash-icon-light.png"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notification.png",
          "color": "#ffffff",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90c3ab39-3a52-4854-b1b9-5a1a504bee30"
      }
    }
  }
}
